#include <stdio.h>
#include <unistd.h>

int main() {
  int PID = getpid();
  int PPID = getppid();
  printf("Start PID=%i, parent PID=%i\n", PID, PPID);
  fork();
  PID = getpid();
  PPID = getppid();
  if (PID)
    printf("PARENT PID=%i, child PID=%i\n", PID, PPID);
  else
    printf("CHILD PID=%i, parent PID=%i\n", PID, PPID);
}
