#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
  char *str = NULL;
  size_t size = 0;
  ssize_t len;

  len = getline(&str, &size, stdin);

  if (len == -1) {
    printf("Error reading line");
    return 1;
  }

  char *s = str;

  char *token;
  token = strtok_r(str, " ", &s);

  printf("Tokens:");

  while (token != NULL) {
    printf("  \n%s", token);
    token = strtok_r(NULL, " ", &s);
  }

  free(str);
  return 0;
}
