#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
  char *str;
  size_t size;
  ssize_t len;

  len = getline(&str, &size, stdin);

  if (len == -1) {
    printf("Error reading line");
    return 1;
  }
  
  char *del = str;

  char *token;
  while ((token = strtok_r(str, " ", &str))) {
    printf("%s\n", token);
    if(str)str = NULL;
  }
  free(del);
  return 0;
}
