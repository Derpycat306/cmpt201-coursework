#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
  char *str;
  size_t size;
  ssize_t len;

  len = getline(&str, &size, stdin);

  if (len == 0) {
    printf("no input provided");
    return 1;
  }
  
  //get first token
  char *token;
  token = strtok_r(str, " ", &str);
  printf("%s\n", token);
  
  //get all other tokens
  while ((token = strtok_r(NULL, " ", &str))) {
    printf("%s\n", token);
  }
}
